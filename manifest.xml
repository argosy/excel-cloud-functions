<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" 
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
           xsi:type="TaskPaneApp">

  <!-- Basic Settings -->
  <Id>a1b2c3d4-e5f6-7890-abcd-ef1234567890</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>CloudFnc</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  
  <!-- Display Information -->
  <DisplayName DefaultValue="Cloud Functions" />
  <Description DefaultValue="Add 1 to selected ranges using Google Cloud Run" />
  
  <!-- Icons -->
  <IconUrl DefaultValue="https://via.placeholder.com/32x32/0078d4/ffffff?text=CF" />
  <HighResolutionIconUrl DefaultValue="https://via.placeholder.com/64x64/0078d4/ffffff?text=CF" />
  
  <!-- Supported Hosts -->
  <Hosts>
    <Host Name="Workbook" />
  </Hosts>
  
  <!-- Requirements -->
  <Requirements>
    <Sets>
      <Set Name="ExcelApi" MinVersion="1.7"/>
    </Sets>
  </Requirements>
  
  <!-- Default Settings -->
  <DefaultSettings>
    <SourceLocation DefaultValue="https://argosy.github.io/excel-cloud-functions/taskpane.html" />
  </DefaultSettings>
  
  <!-- Permissions -->
  <Permissions>ReadWriteDocument</Permissions>

</OfficeApp>